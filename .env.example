# GitHub OAuth App Credentials
GITHUB_CLIENT_ID="YOUR_GITHUB_CLIENT_ID"
GITHUB_CLIENT_SECRET="YOUR_GITHUB_CLIENT_SECRET"

# GitHub Repository Configuration
GITHUB_REPO_OWNER="YOUR_GITHUB_USERNAME_OR_ORG"
GITHUB_REPO_NAME="YOUR_GITHUB_REPOSITORY_NAME"
# Optional: Path within the repo to store posts, defaults to 'posts'
GITHUB_POSTS_PATH="posts"

# Authentication Secret (generate a strong random string)
# openssl rand -base64 32
AUTH_SECRET="YOUR_STRONG_RANDOM_SECRET"

# Base URL of your deployment
NEXT_PUBLIC_BASE_URL="http://localhost:9002"
